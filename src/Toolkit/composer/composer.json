{
	"name" : "pqe/pqe-admin",
	"type" : "project",
	"description" : "PQE Admin System",
	"keywords" : [
		"framework",
		"laravel",
		"pqe"
	],
	"license" : "MIT",
	"require" : {
		"php" : "^8.2",
		"bugsnag/bugsnag-laravel" : "^2.26",
		"directorytree/ldaprecord-laravel" : "*",
		"doctrine/dbal" : "^3.7",
		"guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.11",
		"laravel/framework" : "^10.10",
        "laravel/fortify": "^1.19",
        "laravel/jetstream": "^4.1",
		"laravel/passport" : "^11.10",
		"laravel/sanctum" : "^3.3",
		"laravel/tinker" : "^2.8",
		"laravel/ui" : "^4.2.2",
		"laraveldaily/laravel-charts" : "^0.2",
		"lcobucci/jwt" : "^5.1",
		"maatwebsite/excel" : "^3.1",
		"nuovo/spreadsheet-reader" : "^0.5.11",
		"orangehill/iseed" : "^3.0",
		"pqe-develop/pqe-admin": "^2.11",
		"predis/predis" : "^2.2",
		"spatie/laravel-ignition" : "^2.3",
		"spatie/laravel-medialibrary" : "^10.15",
		"symfony/http-client" : "^6.3",
		"symfony/postmark-mailer" : "^6.3",
		"tightenco/ziggy": "^1.0"
		"yajra/laravel-datatables-oracle" : "^10.11"
	},
	"require-dev" : {
		"kitloong/laravel-migrations-generator" : "^6.11",
		"fakerphp/faker" : "^1.23",
        "laravel/breeze": "^1.26",
		"laravel/dusk" : "^7.11",
		"mockery/mockery" : "^1.6",
		"nunomaduro/collision" : "^7.0",
		"phpunit/phpunit" : "^10.1",
		"reliese/laravel" : "^1.2",
        "spatie/laravel-ignition": "^2.0"
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "app/",
			"Database\\Factories\\" : "database/factories/",
			"Database\\Seeders\\" : "database/seeders/",
			"Pqe\\Admin\\" : "packages/pqe-develop/pqe-admin/src",
			"Pqe\\Admin\\Database\\Seeders\\" : "packages/pqe-develop/pqe-admin/src/database/seeders"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Tests\\" : "tests/"
		}
	},
	"scripts" : {
		"post-autoload-dump" : [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi"
		],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
	},
	"config" : {
		"optimize-autoloader" : true,
		"preferred-install" : "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "platform": {
            "php": "8.2"
        }
	},
    "minimum-stability": "stable",
	"prefer-stable" : true
}
